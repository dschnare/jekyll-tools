---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>This is real law</title>
	<link href="http://www.thisisreallaw.com/atom.xml" rel="self"/>
	<link href="http://www.thisisreallaw.com/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.thisisreallaw.com/</id>
	<author>
		<name>Lexis-Nexis</name>
		<email>webmaster@thisisreallaw.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://www.thisisreallaw.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://www.thisisreallaw.com{{ post.url }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>